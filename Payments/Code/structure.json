    "query": {
        "from": {
            "table": "Paypal\\Payments\\Code\\Tables\\Paypal",
            "alias": "fpp",
            "name": "#__paypal_payments"
        },
        "select": [
            "fpp.id",
            "fpp.payment_id",
            "fp.code AS payment_id_code",
            "fpp.transaction_code",
            "fpp.status",
            "fpp.amount",
            "fpp.currency",
            "fpp.item_number",
            "fpp.created_by",
            "uu.name AS created_by_name",
            "fpp.date_created",
            "fpp.modified_by",
            "uu_4245.name AS modified_by_name",
            "fpp.date_modified"
        ],
        "leftjoin": [
            {
                "table": "Paypal\\Payments\\Code\\Tables\\Payments",
                "alias": "fp",
                "name": "#__paypal_payments",
                "on": "fpp.payment_id = fp.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu",
                "name": "#__users_users",
                "on": "fpp.created_by = uu.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu_4245",
                "name": "#__users_users",
                "on": "fpp.modified_by = uu_4245.id"
            }
        ]
    }
